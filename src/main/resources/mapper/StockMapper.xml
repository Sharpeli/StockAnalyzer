<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.spring.stockAnalyzer.infrastructure.mybatis.mapper.StockMapper">
    <insert id="insert">
        insert into stocks(id, name, description)
        values(
                  #{stock.id},
                  #{stock.name},
                  #{stock.description})
    </insert>
</mapper>